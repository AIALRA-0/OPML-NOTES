            <outline text="首先断开负载并施加一个电流源iTEST" _mubu_text="%3Cspan%3E%E9%A6%96%E5%85%88%E6%96%AD%E5%BC%80%E8%B4%9F%E8%BD%BD%E5%B9%B6%E6%96%BD%E5%8A%A0%E4%B8%80%E4%B8%AA%E7%94%B5%E6%B5%81%E6%BA%90iTEST%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22iYeaKnCAk9%22%2C%22oh%22%3A131%2C%22ow%22%3A314%2C%22uri%22%3A%22document_image%2F4238704_9dd632ad-7157-4150-f4d0-8f6a0ce99db4.png%22%7D%5D"/>
