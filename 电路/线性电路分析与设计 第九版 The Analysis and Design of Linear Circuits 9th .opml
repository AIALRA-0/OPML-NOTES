        <outline text="最大功率：接口处传递的功率等于 υ 乘以 i 的乘积" _mubu_text="%3Cspan%3E%E6%9C%80%E5%A4%A7%E5%8A%9F%E7%8E%87%EF%BC%9A%E6%8E%A5%E5%8F%A3%E5%A4%84%E4%BC%A0%E9%80%92%E7%9A%84%E5%8A%9F%E7%8E%87%E7%AD%89%E4%BA%8E%20%CF%85%20%E4%B9%98%E4%BB%A5%20i%20%E7%9A%84%E4%B9%98%E7%A7%AF%3C/span%3E" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%22ZBFvEIHvtb%22%2C%22oh%22%3A105%2C%22ow%22%3A171%2C%22uri%22%3A%22document_image%2F4238704_7390c84e-5973-457f-8eca-b64e571feb94.png%22%7D%5D"/>
