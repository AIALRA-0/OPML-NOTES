        <outline text="固定源接口规则" _mubu_text="%3Cspan%3E%E5%9B%BA%E5%AE%9A%E6%BA%90%E6%8E%A5%E5%8F%A3%E8%A7%84%E5%88%99%3C/span%3E" _note="" _mubu_note="">
          <outline text="可用的最大电压是开路电压" _mubu_text="%3Cspan%3E%E5%8F%AF%E7%94%A8%E7%9A%84%E6%9C%80%E5%A4%A7%E7%94%B5%E5%8E%8B%E6%98%AF%E5%BC%80%E8%B7%AF%E7%94%B5%E5%8E%8B%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="可用的最大电流是短路电流" _mubu_text="%3Cspan%3E%E5%8F%AF%E7%94%A8%E7%9A%84%E6%9C%80%E5%A4%A7%E7%94%B5%E6%B5%81%E6%98%AF%E7%9F%AD%E8%B7%AF%E7%94%B5%E6%B5%81%3C/span%3E" _note="" _mubu_note=""/>
